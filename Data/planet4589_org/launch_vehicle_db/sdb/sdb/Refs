FORMAT  = 'AF1     '                       / AF1 File Format Aug 1996
EXTNAME = 'Launch_Refs'                    / File Name
NAXIS1  =                    141           / Width of table in bytes
TFIELDS =                      2           / Number of fields
TTYPE1  = 'Cite    '                       / Citation tag
TDISP1  = 'A20     '                       / Display format
TPOS1   =                      1           / Byte start position
TTYPE2  = 'Reference'                      / Full reference
TDISP2  = 'A120    '                       / Display format
TPOS2   =                     22           / Byte start position
END

